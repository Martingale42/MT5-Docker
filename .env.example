# VNC Password Configuration
VNC_PASSWORD=mt5pass

# MT5 Watchdog Configuration
# Enable automatic restart if MT5 becomes unresponsive
MT5_WATCHDOG_ENABLED=true
# Health check interval in seconds
MT5_WATCHDOG_INTERVAL=30
# Number of consecutive failures before restart
MT5_WATCHDOG_MAX_RETRIES=3

# Grafana Configuration
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin
